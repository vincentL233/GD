) vars.get(\"vy\").setNumber((Math.random() - 0.5) * 2);","if (!vars.has(\"tick\")) vars.get(\"tick\").setNumber(new Date().getTime());","if (!vars.has(\"pause\")) vars.get(\"pause\").setNumber(0);","if (!vars.has(\"sleeping\")) vars.get(\"sleeping\").setNumber(0); ","if (!vars.has(\"moveTime\")) vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","if (!vars.has(\"movePattern\")) vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","if (!vars.has(\"curveAngle\")) vars.get(\"curveAngle\").setNumber(0);","if (!vars.has(\"targetX\")) vars.get(\"targetX\").setNumber(cat.getX() + Math.random() * 200 - 100);","if (!vars.has(\"targetY\")) vars.get(\"targetY\").setNumber(cat.getY() + Math.random() * 100 - 50);","","let vx = vars.get(\"vx\").getAsNumber();","let vy = vars.get(\"vy\").getAsNumber();","let paused = vars.get(\"pause\").getAsNumber();","let sleeping = vars.get(\"sleeping\").getAsNumber();","let moveTime = vars.get(\"moveTime\").getAsNumber();","let movePattern = vars.get(\"movePattern\").getAsNumber();","let curveAngle = vars.get(\"curveAngle\").getAsNumber();","let targetX = vars.get(\"targetX\").getAsNumber();","let targetY = vars.get(\"targetY\").getAsNumber();","","const lastTick = vars.get(\"tick\").getAsNumber();","const now = new Date().getTime();","const speed = 80 / 60;","const elapsedTime = now - lastTick;","","if (","  runtimeScene.getVariables().get(\"catTaskTriggered\").getAsNumber() === 1 &&","  runtimeScene.getVariables().get(\"catTaskCompleted\").getAsNumber() === 1",") {","  return;","}","","// 睡覺階段","if (sleeping === 1) {","  cat.setAnimationName(\"getDown\");","  if (elapsedTime > 3000) {","    vars.get(\"sleeping\").setNumber(0);","    vars.get(\"pause\").setNumber(0);","    vars.get(\"tick\").setNumber(now);","    vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","    vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","    vars.get(\"targetX\").setNumber(cat.getX() + (Math.random() * 300 - 150));","    vars.get(\"targetY\").setNumber(cat.getY() + (Math.random() * 100 - 50));","  }","  return;","}","","// 坐下階段","if (paused === 1) {","  cat.setAnimationName(\"sitdown\");","  if (elapsedTime > 1000) {","    vars.get(\"pause\").setNumber(0);","    vars.get(\"tick\").setNumber(now);","    vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","    vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","    vars.get(\"targetX\").setNumber(cat.getX() + (Math.random() * 300 - 150));","    vars.get(\"targetY\").setNumber(cat.getY() + (Math.random() * 100 - 50));","  }","  return;","}","","// 改變移動方式","if (elapsedTime > moveTime) {","  const chance = Math.random();","  if (chance < 0.25) {","    vars.get(\"sleeping\").setNumber(1);","    vars.get(\"tick\").setNumber(now);","    return;","  } else if (chance < 0.5) {","    vars.get(\"pause\").setNumber(1);","    vars.get(\"tick\").setNumber(now);","    return;","  } else {","    movePattern = Math.floor(Math.random() * 3);","    vars.get(\"movePattern\").setNumber(movePattern);","    vars.get(\"moveTime\").setNumber(Math.random() * 4000 + 1000);","    targetX = cat.getX() + (Math.random() * 400 - 200);","    targetY = cat.getY() + (Math.random() * 150 - 75);","    vars.get(\"targetX\").setNumber(targetX);","    vars.get(\"targetY\").setNumber(targetY);","","    const dx = targetX - cat.getX();","    const dy = targetY - cat.getY();","    const dist = Math.sqrt(dx * dx + dy * dy);","    if (dist > 0) {","      vx = (dx / dist) * (Math.random() * 1 + 0.8);","      vy = (dy / dist) * (Math.random() * 0.8 + 0.5);","    } else {","      vx = (Math.random() - 0.5) * 1.8;","      vy = (Math.random() - 0.5) * 1.2;","    }","    vars.get(\"vx\").setNumber(vx);","    vars.get(\"vy\").setNumber(vy);","    vars.get(\"tick\").setNumber(now);","    vars.get(\"curveAngle\").setNumber(0);","  }","}","","// 自主移動中","cat.setAnimationName(\"walk\");","","if (movePattern === 0) {","  cat.flipX(vx < 0);"