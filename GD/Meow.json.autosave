vior(\"Tween\") &&","    catTeaser.getBehavior(\"Tween\").isPlaying(\"sway\")) {","  // teaser 激活時跟隨 play point","  const playX = catTeaser.getPointX(\"play\");","  const playY = catTeaser.getPointY(\"play\");","","  const dx = playX - cat.getX();","  const dy = playY - cat.getY();","  const dist = Math.sqrt(dx * dx + dy * dy);","","  if (dist > 1) {","    const speed = 80 / 60;","    const vx = (dx / dist) * speed;","    const vy = (dy / dist) * speed;","","    cat.setAnimationName(\"walk\");","    cat.flipX(vx < 0);","    cat.setX(cat.getX() + vx);","    cat.setY(cat.getY() + vy);","  }","","  return; // teaser 任務優先處理，阻擋其他移動","}","","const vars = cat.getVariables();","","// 初始化變數","if (!vars.has(\"vx\")) vars.get(\"vx\").setNumber((Math.random() - 0.5) * 2);","if (!vars.has(\"vy\")) vars.get(\"vy\").setNumber((Math.random() - 0.5) * 2);","if (!vars.has(\"tick\")) vars.get(\"tick\").setNumber(new Date().getTime());","if (!vars.has(\"pause\")) vars.get(\"pause\").setNumber(0);","if (!vars.has(\"sleeping\")) vars.get(\"sleeping\").setNumber(0); ","if (!vars.has(\"moveTime\")) vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","if (!vars.has(\"movePattern\")) vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","if (!vars.has(\"curveAngle\")) vars.get(\"curveAngle\").setNumber(0);","if (!vars.has(\"targetX\")) vars.get(\"targetX\").setNumber(cat.getX() + Math.random() * 200 - 100);","if (!vars.has(\"targetY\")) vars.get(\"targetY\").setNumber(cat.getY() + Math.random() * 100 - 50);","","let vx = vars.get(\"vx\").getAsNumber();","let vy = vars.get(\"vy\").getAsNumber();","let paused = vars.get(\"pause\").getAsNumber();","let sleeping = vars.get(\"sleeping\").getAsNumber();","let moveTime = vars.get(\"moveTime\").getAsNumber();","let movePattern = vars.get(\"movePattern\").getAsNumber();","let curveAngle = vars.get(\"curveAngle\").getAsNumber();","let targetX = vars.get(\"targetX\").getAsNumber();","let targetY = vars.get(\"targetY\").getAsNumber();","","const lastTick = vars.get(\"tick\").getAsNumber();","const now = new Date().getTime();","const speed = 80 / 60;","const elapsedTime = now - lastTick;","","if (","  runtimeScene.getVariables().get(\"catTaskTriggered\").getAsNumber() === 1 &&","  runtimeScene.getVariables().get(\"catTaskCompleted\").getAsNumber() === 1",") {","  return;","}","","// 睡覺階段","if (sleeping === 1) {","  cat.setAnimationName(\"getDown\");","  if (elapsedTime > 3000) {","    vars.get(\"sleeping\").setNumber(0);","    vars.get(\"pause\").setNumber(0);","    vars.get(\"tick\").setNumber(now);","    vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","    vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","    vars.get(\"targetX\").setNumber(cat.getX() + (Math.random() * 300 - 150));","    vars.get(\"targetY\").setNumber(cat.getY() + (Math.random() * 100 - 50));","  }","  return;","}","","// 坐下階段","if (paused === 1) {","  cat.setAnimationName(\"sitdown\");","  if (elapsedTime > 1000) {","    vars.get(\"pause\").setNumber(0);","    vars.get(\"tick\").setNumber(now);","    vars.get(\"moveTime\").setNumber(Math.random() * 3000 + 2000);","    vars.get(\"movePattern\").setNumber(Math.floor(Math.random() * 3));","    vars.get(\"targetX\").setNumber(cat.getX() + (Math.random() * 300 - 150));","    vars.get(\"targetY\").setNumber(cat.getY() + (Math.random() * 100 - 50));","  }","  return;","}","","// 改變移動方式","if (elapsedTime > moveTime) {","  const chance = Math.random();","  if (chance < 0.25) {","    vars.get(\"sleeping\").setNumber(1);","    vars.get(\"tick\").setNumber(now);","    return;","  } else if (chance < 0.5) {","    vars.get(\"pause\").setNumber(1);","    vars.get(\"tick\").setNumber(now);","    return;","  } else {","    movePattern = Math.floor(Math.random() * 3);","    vars.get(\"movePattern\").setNumber(movePattern);","    vars.get(\"moveTime\").setNumber(Math.random() * 4000 + 1000);","    targetX = cat.getX() + (Math.random() * 400 - 200);",