() 來計算時間差","  const currentTime = Date.now();","  const lastUpdateTime = runtimeScene.getVariables().get(\"lastProgressUpdateTime\").getAsNumber();","  const elapsedTimeInSeconds = (currentTime - lastUpdateTime) / 1000;","  ","  // 限制單次時間差，防止大幅度下降","  const limitedTimeDelta = Math.min(elapsedTimeInSeconds, 0.1);","  ","  // 計算新的進度值（每秒減少5%）","  const newValue = Math.max(0, currentValue - (5 * limitedTimeDelta));","  ","  // 更新進度值和最後更新時間","  runtimeScene.getVariables().get(\"progressCurrentValue\").setNumber(newValue);","  runtimeScene.getVariables().get(\"lastProgressUpdateTime\").setNumber(currentTime);","  ","  // 根據進度值計算新寬度","  const newWidth = (newValue / 100) * maxWidth;","  ","  // 設置進度條寬度","  progressBar.setWidth(newWidth);","  ","  // 調試輸出","  if (Math.floor(currentValue) !== Math.floor(newValue)) {","    // 只在整數值變化時輸出，避免過多日誌","    console.log(\"進度: \" + newValue.toFixed(1) + \"%, 寬度: \" + newWidth.toFixed(1) + \"px\");","  }","  ","  // 如果進度值降至0，可以執行額外操作","  if (newValue <= 0 && currentValue > 0) {","    console.log(\"進度條歸零!\");","    // 這裡可以添加進度條歸零時的額外處理代碼","  }","}","","// 監聽遊戲視窗大小變化（如果需要）","if (runtimeScene.getVariables().has(\"lastGameWidth\")) {","  const lastWidth = runtimeScene.getVariables().get(\"lastGameWidth\").getAsNumber();","  const currentWidth = runtimeScene.getGame().getGameResolutionWidth();","  ","  // 如果遊戲寬度變化，重新計算進度條最大寬度","  if (lastWidth !== currentWidth) {","    const newMaxWidth = currentWidth * 0.8; // 可以調整這個比例","    runtimeScene.getVariables().get(\"progressMaxWidth\").setNumber(newMaxWidth);","    ","    // 重新計算並應用當前寬度","    const currentValue = runtimeScene.getVariables().get(\"progressCurrentValue\").getAsNumber();","    const newWidth = (currentValue / 100) * newMaxWidth;","    if (progressBar) {","      progressBar.setWidth(newWidth);","    }","    console.log(\"遊戲視窗大小變化! 新寬度: \" + currentWidth + \"px, 進度條最大寬度: \" + newMaxWidth + \"px\");","  }","  ","  // 更新上次寬度","  runtimeScene.getVariables().get(\"lastGameWidth\").setNumber(