onsole.log(\"任務完成，將在1秒後結束交互階段\");","    }","  } else {","    console.log(\"已達到最大狀態，保持在 feed_boel3\");","  }","}","","// 處理任務完成後的延遲","if (runtimeScene.getVariables().has(\"taskCompleted\") && ","    runtimeScene.getVariables().get(\"taskCompleted\").getAsNumber() === 1) {","  ","  const completionTime = runtimeScene.getVariables().get(\"completionTime\").getAsNumber();","  ","  if (currentTime > completionTime) {","    runtimeScene.getVariables().get(\"bowlInteractionActive\").setNumber(0);","    runtimeScene.getVariables().get(\"taskCompleted\").setNumber(0);","    ","    // 隱藏進度條","    if (progressBar) {","      progressBar.setOpacity(0);","    }","    ","    // 最後一次設置eventCompleted為1，確保整個任務完成後進度條會重置 - 新增部分","    runtimeScene.getVariables().get(\"eventCompleted\").setNumber(1);","    console.log(\"任務完全結束，設置eventCompleted=1\");","    ","    console.log(\"食物碗已填滿，交互階段完全結束\");","  }","}","","// 當計時器重置時，重置事件觸發狀態","if (counttime >= 60) {","  runtimeScene.getVariables().get(\"bowlEventTriggered\").setNumber(0);","  runtimeScene.getVariables().get(\"bowlInteractionActive\").setNumber(0);","  runtimeScene.getVariables().get(\"taskShown\").setNumber(0);","  if (runtimeScene.getVariables().has(\"taskCompleted\")) {","    runtimeScene.getVariables().get(\"taskCompleted\").setNumber(0);","  }","  ","  // 將Task物件和進度條設回完全透明","  if (taskObject) {","    taskObject.setOpacity(0);","  }","  if (progressBar) {","    progressBar.setOpacity(0);","  }","  ","  // 重置食物碗狀態","  runtimeScene.getVariables().get(\"bowlState\").setNumber(0);","  if (feedBowl) {","    feedBowl.setAnimationName(\"feed_boel0\");","  }","  ","  const newRandomTriggerTime = Math.floor(Math.random() * (60 - 10 + 1)) + 10;","  runtimeScene.getVariables().get(\"bowlRandomTriggerTime\").setNumber(newRandomTriggerTime);","  console.log(\"重設食物碗隨機事件，下次將在倒數 \" + newRandomTriggerTime + \" 秒時觸發\");","}","","// 根據交互狀態更改食物碗外觀（可選）","if (interactionActive === 1) {","  // 當可點擊時的視覺效果","  feedBowl.setOpacity(255);  // 完全不透明","} else